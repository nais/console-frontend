{
	"name": "console-frontend",
	"version": "0.0.1",
	"private": true,
	"packageManager": "npm@11.6.1",
	"scripts": {
		"prepare": "svelte-kit sync || echo ''",
		"allow-scripts": "allow-scripts",
		"dev": "vite dev",
		"dev-live-share": "vite dev --host ::",
		"build": "vite build",
		"preview": "vite preview",
		"check": "npx houdini generate && svelte-check --tsconfig ./tsconfig.json --fail-on-warnings",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"eslint": "eslint .",
		"format": "prettier --write .",
		"check-outdated": "node check-updates-cooldown.mjs",
		"update-outdated": "node check-updates-cooldown.mjs --install",
		"check-age": "node check-package-age.mjs",
		"install-safe": "npx npq install",
		"test": "vitest",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"lint-staged": "lint-staged",
		"lockfile-lint": "lockfile-lint --path package-lock.json",
		"unused": "node find-unused-gql.mjs && echo '---' && node find-missing-css-vars.mjs"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^4.1.3",
		"@eslint/compat": "^2.0.0",
		"@eslint/js": "^9.39.2",
		"@lavamoat/allow-scripts": "^3.4.1",
		"@lavamoat/preinstall-always-fail": "^2.1.1",
		"@layerstack/utils": "^2.0.0-next.18",
		"@nais/ds-svelte-community": "^2.0.0-next.6",
		"@storybook/addon-docs": "10.1.10",
		"@storybook/addon-svelte-csf": "^5.0.10",
		"@storybook/addon-themes": "10.1.10",
		"@storybook/sveltekit": "10.1.10",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "^2.49.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.18",
		"@types/d3-array": "^3.2.2",
		"@types/d3-time": "^3.0.4",
		"@types/luxon": "^3.7.1",
		"@types/sortablejs": "^1.15.9",
		"date-fns": "^4.1.0",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-storybook": "10.1.10",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"houdini": "^2.0.0-next.11",
		"houdini-svelte": "^3.0.0-next.13",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"lockfile-lint": "^4.14.1",
		"npm-check-updates": "^19.2.0",
		"npq": "^3.16.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"storybook": "10.1.10",
		"svelte": "^5.46.0",
		"svelte-check": "^4.3.5",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.50.0",
		"vite": "^7.3.0",
		"vitest": "^4.0.16"
	},
	"type": "module",
	"dependencies": {
		"@codemirror/language": "^6.11.3",
		"@codemirror/state": "^6.5.2",
		"@codemirror/theme-one-dark": "^6.1.3",
		"@codemirror/view": "^6.39.4",
		"@prometheus-io/codemirror-promql": "^0.308.0",
		"@slack/types": "^2.19.0",
		"@slack/web-api": "^7.13.0",
		"cron-parser": "^5.4.0",
		"cronstrue": "^3.9.0",
		"eslint-plugin-unicorn": "^62.0.0",
		"graphql": "^16.12.0",
		"graphql-sse": "^2.6.0",
		"layerchart": "^2.0.0-next.43",
		"luxon": "^3.7.2",
		"pretty-bytes": "^7.1.0",
		"sortablejs": "^1.15.6",
		"svelte-highlight": "^7.9.0"
	},
	"overrides": {
		"@sveltejs/kit": {
			"cookie": "^0.7.0"
		}
	},
	"lint-staged": {
		"*.{js,ts,svelte,gql,css,md,json}": "prettier --write",
		"*.{js,ts,svelte,gql}": [
			"eslint --fix",
			"svelte-check --tsconfig ./tsconfig.json --fail-on-warnings"
		]
	},
	"graphql": {
		"schema": "https://localhost:3000/graphql",
		"documents": "**/*.{graphql,js,ts,jsx,tsx,svelte}"
	},
	"lavamoat": {
		"allowScripts": {
			"@lavamoat/preinstall-always-fail": false,
			"vite>esbuild": true
		}
	}
}
