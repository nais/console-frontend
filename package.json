{
	"name": "console-frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prepare": "husky && svelte-kit sync || echo ''",
		"dev": "vite dev",
		"dev-live-share": "vite dev --host ::",
		"build": "vite build",
		"preview": "vite preview",
		"check": "npx houdini generate && svelte-check --tsconfig ./tsconfig.json --fail-on-warnings",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"eslint": "eslint .",
		"format": "prettier --write .",
		"test": "vitest",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"lint-staged": "lint-staged"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^4.0.1",
		"@eslint/compat": "^1.3.1",
		"@nais/ds-svelte-community": "^1.0.1",
		"@navikt/ds-css": "7.22.0",
		"@storybook/addon-svelte-csf": "^5.0.4",
		"@storybook/addon-themes": "^9.0.15",
		"@storybook/sveltekit": "^9.0.15",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "2.21.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/amplitude-js": "^8.16.5",
		"@types/luxon": "^3.6.2",
		"@typescript-eslint/eslint-plugin": "^8.35.1",
		"@typescript-eslint/parser": "^8.35.1",
		"date-fns": "^4.1.0",
		"eslint": "^9.30.1",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-storybook": "^9.0.15",
		"eslint-plugin-svelte": "^3.10.1",
		"houdini": "^2.0.0-next.0",
		"houdini-svelte": "^2.2.0-next.1",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.2",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"storybook": "^9.0.15",
		"svelte": "5.31.1",
		"svelte-check": "^4.2.2",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.35.1",
		"vite": "^6.3.5",
		"vitest": "^3.2.4",
		"@storybook/addon-docs": "^9.0.15"
	},
	"type": "module",
	"dependencies": {
		"@slack/types": "^2.14.0",
		"@slack/web-api": "^7.9.3",
		"amplitude-js": "^8.21.9",
		"bytes-iec": "^3.1.1",
		"cron-parser": "^5.3.0",
		"cronstrue": "^3.0.0",
		"echarts": "^5.6.0",
		"eslint-plugin-unicorn": "^59.0.1",
		"graphql": "^16.11.0",
		"graphql-sse": "^2.5.4",
		"luxon": "^3.6.1",
		"pretty-bytes": "^7.0.0",
		"svelte-floating-ui": "^1.6.2",
		"svelte-highlight": "^7.8.3"
	},
	"overrides": {
		"@sveltejs/kit": {
			"cookie": "^0.7.0"
		}
	},
	"lint-staged": {
		"*.{js,ts,svelte,gql,css,md,json}": "prettier --write .",
		"*.{js,ts,svelte,gql}": [
			"eslint --fix .",
			"svelte-check --tsconfig ./tsconfig.json --fail-on-warnings"
		]
	},
	"graphql": {
		"schema": "https://localhost:3000/graphql",
		"documents": "**/*.{graphql,js,ts,jsx,tsx,svelte}"
	}
}
