{
	"name": "console-frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prepare": "husky && svelte-kit sync || echo ''",
		"dev": "vite dev",
		"dev-live-share": "vite dev --host ::",
		"build": "vite build",
		"preview": "vite preview",
		"check": "npx houdini generate && svelte-check --tsconfig ./tsconfig.json --fail-on-warnings",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"eslint": "eslint .",
		"format": "prettier --write .",
		"test": "vitest",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"lint-staged": "lint-staged"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^4.1.1",
		"@eslint/compat": "^1.3.2",
		"@nais/ds-svelte-community": "^1.0.2",
		"@navikt/ds-css": "7.22.0",
		"@storybook/addon-docs": "^9.1.7",
		"@storybook/addon-svelte-csf": "^5.0.8",
		"@storybook/addon-themes": "^9.1.7",
		"@storybook/sveltekit": "^9.1.7",
		"@sveltejs/adapter-node": "^5.3.2",
		"@sveltejs/kit": "2.42.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@types/amplitude-js": "^8.16.5",
		"@types/luxon": "^3.7.1",
		"@types/sortablejs": "^1.15.8",
		"@typescript-eslint/eslint-plugin": "^8.44.0",
		"@typescript-eslint/parser": "^8.44.0",
		"date-fns": "^4.1.0",
		"eslint": "^9.35.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-storybook": "^9.1.7",
		"eslint-plugin-svelte": "^3.12.3",
		"houdini": "^2.0.0-next.10",
		"houdini-svelte": "^3.0.0-next.13",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"storybook": "^9.1.7",
		"svelte": "5.39.2",
		"svelte-check": "^4.3.1",
		"tslib": "^2.8.1",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.44.0",
		"vite": "^7.1.6",
		"vitest": "^3.2.4"
	},
	"type": "module",
	"dependencies": {
		"@codemirror/language": "^6.11.3",
		"@codemirror/state": "^6.5.2",
		"@codemirror/theme-one-dark": "^6.1.3",
		"@codemirror/view": "^6.38.2",
		"@prometheus-io/codemirror-promql": "^0.306.0-rc.1",
		"@slack/types": "^2.16.0",
		"@slack/web-api": "^7.10.0",
		"amplitude-js": "^8.21.9",
		"bytes-iec": "^3.1.1",
		"cron-parser": "^5.4.0",
		"cronstrue": "^3.3.0",
		"echarts": "^6.0.0",
		"eslint-plugin-unicorn": "^61.0.2",
		"graphql": "^16.11.0",
		"graphql-sse": "^2.5.4",
		"luxon": "^3.7.2",
		"pretty-bytes": "^7.0.1",
		"sortablejs": "^1.15.6",
		"svelte-floating-ui": "^1.6.2",
		"svelte-highlight": "^7.8.4"
	},
	"overrides": {
		"@sveltejs/kit": {
			"cookie": "^0.7.0"
		}
	},
	"lint-staged": {
		"*.{js,ts,svelte,gql,css,md,json}": "prettier --write .",
		"*.{js,ts,svelte,gql}": [
			"eslint --fix .",
			"svelte-check --tsconfig ./tsconfig.json --fail-on-warnings"
		]
	},
	"graphql": {
		"schema": "https://localhost:3000/graphql",
		"documents": "**/*.{graphql,js,ts,jsx,tsx,svelte}"
	}
}
